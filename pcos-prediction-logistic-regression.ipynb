from warnings import filterwarnings
filterwarnings("ignore")

#read datasets
df_inf=pd.read_csv("PCOS_infertility.csv", encoding='iso-8859-1')
df_noinf=pd.read_csv("data without infertility _final.csv", encoding='iso-8859-1')
